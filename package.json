{
  "name": "ngx-kjua-library",
  "description": "ngx-kjua library builder",
  "version": "2.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "test:lib": "ng test ngx-kjua",
    "test:lib:ci": "npm run test:lib -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "package": "ng-packagr -p ng-package.json && cd dist/ && npm pack",
    "publish": "ng build ngx-kjua --configuration production && npm publish ./dist/ngx-kjua && npm i ngx-kjua@latest"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/werthdavid/ngx-kjua"
  },
  "authors": [
    {
      "name": "David Werth",
      "email": "werth.david@gmail.com"
    }
  ],
  "keywords": [
    "angular",
    "kjua",
    "qr-code",
    "qr",
    "svg"
  ],
  "bugs": {
    "url": "https://github.com/werthdavid/ngx-kjua/issues"
  },
  "dependencies": {
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/compiler-cli": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/forms": "^14.2.1",
    "@angular/language-service": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "kjua-svg": "^1.13.1",
    "ngx-kjua": "^1.14.4",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular/cli": "^14.2.2",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.7.16",
    "core-js": "^3.25.1",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.1",
    "rxjs": "~7.5.6",
    "ts-node": "~10.5.0",
    "tslint": "~6.1.3",
    "typescript": "~4.8.3"
  }
}
